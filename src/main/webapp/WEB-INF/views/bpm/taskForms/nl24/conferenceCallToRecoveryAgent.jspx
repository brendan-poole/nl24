<div xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:form="http://www.springframework.org/tags/form"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:spring="http://www.springframework.org/tags" version="2.0">

	<ul class="basictab">
		<li id="selectedtab">
			<button class="tabbutton" name="page" value="recoveryAgent">Recovery agent details</button>
		</li>
	</ul>
	<div class="tabcontent">
		Name: ${command.processInstance.data.recoveryAgentInstruction.recoveryAgent.name}<br/>
		Telephone: ${command.processInstance.data.recoveryAgentInstruction.recoveryAgent.telephone}

	</div>
	<div style="float:inherit; clear: both; height: 0px;"/>
</div>
