<div xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:form="http://www.springframework.org/tags/form"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:spring="http://www.springframework.org/tags" version="2.0">

	<ul class="basictab">
		<li id="selectedtab">
			<button class="tabbutton" name="page" value="thirdParty">Third party</button>
		</li>
	</ul>
	<div class="tabcontent">
		Name: ${command.processInstance.data.thirdParty.contact.forename} ${command.processInstance.data.thirdParty.contact.surname}<br/>
		Mobile telephone: ${command.processInstance.data.thirdParty.contact.mobileTelephone}<br/>
		Home telephone: ${command.processInstance.data.thirdParty.contact.homeTelephone}<br/>
		Work telephone: ${command.processInstance.data.thirdParty.contact.workTelephone}<br/>

	</div>
	<div style="float:inherit; clear: both; height: 0px;"/>
</div>
